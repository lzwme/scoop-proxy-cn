{
  "json.schemas": [
    {
      "url": "https://ghproxy.net/https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
      "fileMatch": [
        ".vscode/Template.jsonc",
        "bucket/*.json"
      ]
    }
  ],
  "powershell.codeFormatting.preset": "OTBS",
  "yaml.format.bracketSpacing": true,
  "yaml.format.enable": true,
  "yaml.format.singleQuote": true,
  "yaml.validate": true,
  "yaml.schemas": {
    "http://json.schemastore.org/appveyor": "appveyor.yml",
    "https://ghproxy.net/https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json": [
      "bucket/*.yml"
    ]
  },
  "terminals.terminals": [
    {
      "name": "Powershell",
      "shellPath": "powershell.exe",
      "shellArgs": [
        "-NoLogo",
        "-NoExit"
      ],
      "focus": true,
      "open": true
    },
    {
      "name": "Powershell Core",
      "shellPath": "pwsh.exe",
      "shellArgs": [
        "-NoLogo",
        "-NoExit"
      ],
      "focus": true,
      "open": true
    },
    {
      "name": "Powershell Core (Preview)",
      "shellPath": "pwsh-preview.exe",
      "shellArgs": [
        "-NoLogo",
        "-NoExit"
      ],
      "focus": true,
      "open": true
    }
  ]
}