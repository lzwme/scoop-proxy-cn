  "bin": [
    "ambiguous_words.exe",
    "classifier_tester.exe",
    "cntraining.exe",
    "combine_tessdata.exe",
    "dawg2wordlist.exe",
    "mftraining.exe",
    "set_unicharset_properties.exe",
    "shapeclustering.exe",
    "tesseract.exe",
    "text2image.exe",
    "unicharset_extractor.exe",
    "wordlist2dawg.exe"
  ],
  "env_set": {
    "TESSDATA_PREFIX": "$persist_dir\\tessdata"
