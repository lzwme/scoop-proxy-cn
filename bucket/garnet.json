    },
    "arm64": {
      "url": "https://github.com/microsoft/garnet/releases/download/v1.0.79/win-arm64-based-readytorun.zip",
      "hash": "3821d39dc9ad4fa9314d656a7e86c1be3517fa76a9a79748d359ff5283066301"
    }
  },
  "extract_dir": "net9.0",
  "post_install": "Remove-Item -Path \"$dir\\Service\" -Recurse -Force -ErrorAction SilentlyContinue",
  "bin": "GarnetServer.exe",
  "persist": [
    "garnet.conf",
    "config"
  ],
  "checkver": {
    "github": "https://github.com/microsoft/garnet"
