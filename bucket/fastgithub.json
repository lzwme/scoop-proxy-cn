{
  "version": "2.1.4",
  "description": "github加速神器，解决github打不开、用户头像无法加载、releases无法上传下载、git-clone、git-pull、git-push失败等问题. ",
  "homepage": "https://github.com/dotnetcore/FastGithub",
  "license": "MIT",
  "depends": "sudo",
  "architecture": {
    "64bit": {
      "url": "https://github.com/dotnetcore/FastGithub/releases/download/2.1.4/fastgithub_win-x64.zip",
      "hash": "b439afc7b730766aed894876696e9aeb411e3b45f60ebdcfdff247890742c0f6"
    }
  },
  "extract_dir": "fastgithub_win-x64",
  "uninstaller": {
    "script": [
      "sudo taskkill /F /IM 'FastGithub.UI.exe' /FI 'status eq running' | Out-Null",
      "sudo \"$dir\\fastgithub.exe\" stop"
    ]
  },
  "bin": [
    "fastgithub.exe",
    "FastGithub.UI.exe"
  ],
  "shortcuts": [
    [
      "FastGithub.UI.exe",
      "FastGithub.UI"
    ]
  ],
  "persist": [
    "appsettings",
    "cacert",
    "logs",
    "appsettings.json"
  ],
  "checkver": "github",
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://github.com/dotnetcore/FastGithub/releases/download/$version/fastgithub_win-x64.zip"
      }
    }
  }
}