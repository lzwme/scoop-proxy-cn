      "pre_install": "foreach ($z in @('soffice', 'unopkg')) { Set-Content \"$dir\\$z.cmd\" \"@`\"$env:ProgramFiles\\LibreOffice\\program\\$z.com`\"  %*\" -Encoding 'utf8' }"
