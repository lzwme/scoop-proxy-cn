    "url": "https://github.com/goatfungus/NMSSaveEditor/blob/master/VERSION.txt",
    "regex": "currentOid\":\"(?<sha>[0-9a-f]{40}).+rawLines\":\\[\"(?<version>\\d+\\.[\\d.]+)"
