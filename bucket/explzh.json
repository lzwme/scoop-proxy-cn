    "$regname = 'ExplzhDir'",
    "$regvalue = Get-ItemProperty -Path \"$regpath\" -Name \"$regname\" -ErrorAction SilentlyContinue",
    "if ($regvalue -eq $null) {",
    "Set-ItemProperty \"$regpath\" -Name \"$regname\" -Value \"$dir\"",
