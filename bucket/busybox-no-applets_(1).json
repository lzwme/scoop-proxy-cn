  "post_install": [
    "if (!(Test-Path \"$env:USERPROFILE\\.profile\")) {",
    "    info 'creating ~/.profile'",
    "@\"",
    "if [ -e ~/.bashrc ]; then",
    "   source ~/.bashrc",
    "fi",
    "\"@ | Out-File \"$env:USERPROFILE\\.profile\" -Encoding oem",
    "}"
  ],
  "checkver": {
    "url": "https://frippery.org/busybox/",
    "regex": ">busybox-w32-FRP-([\\w-]+)\\."
  },
  "autoupdate": {
