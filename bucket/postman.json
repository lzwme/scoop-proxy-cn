  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://dl.pstmn.io/download/$version/Postman-win64-$version-Setup.exe#/dl.7z",
        "hash": {
          "url": "https://dl.pstmn.io/RELEASES?platform=win64&from=$version",
          "regex": "(?sm)$basename.*?$sha1"
        }
      }
    }
  }
