      "hash": "3f1dff77988b0fb6c4dc40d083ccdee8cf2a6115b7e3190eb6d7621af34b937a",
      "bin": [
        [
          "snes9x-x64.exe",
          "snes9x"
        ]
      ],
      "shortcuts": [
