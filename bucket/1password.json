    [
      "1Password.exe",
      "1Password"
    ]
  ],
  "checkver": {
    "url": "https://app-updates.agilebits.com/product_history/OPW8",
    "regex": "<a href=\".*?\" title=\"([\\d.]+)* - build"
  },
