  "hash": "f369982e7d2990d88d9297663cc21dab71e5f71803221575adb760fb91d4f9ad",
  "extract_to": "IDE",
  "installer": {
    "script": [
      "& \"$bucketsdir\\$bucket\\scripts\\jetbrains\\portable.ps1\" $dir $persist_dir",
