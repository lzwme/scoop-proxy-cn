  "innosetup": true,
  "installer": {
    "script": [
      "# handle runtime cache",
      "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name spc) scripts/AppsUtils.psm1)",
      "Mount-ExternalRuntimeData -Source \"$persist_dir\\appdata\" -Target \"$env:APPDATA\\karing\"",
      "Remove-Module -Name AppsUtils"
    ]
  },
  "uninstaller": {
    "script": [
      "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name spc) scripts/AppsUtils.psm1)",
      "Dismount-ExternalRuntimeData -Target \"$env:APPDATA\\karing\"",
      "Remove-Module -Name AppsUtils"
    ]
  },
  "bin": "karing.exe",
  "shortcuts": [
    [
      "karing.exe",
      "karing"
    ]
  ],
  "checkver": {
    "url": "https://api.github.com/repos/KaringX/karing/releases",
    "jp": "$[0].assets[*].browser_download_url",
    "regex": "/releases/download/v([\\d.]+)/karing_([\\d.]+)_windows"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://ghfast.top/https://github.com/KaringX/karing/releases/download/v$version/karing_$version_windows_x64.exe"
