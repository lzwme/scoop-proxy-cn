{
  "version": "9.1",
  "homepage": "https://www.pgadmin.org/",
  "description": "PostgreSQL administration and development platform.",
  "license": "PostgreSQL",
  "architecture": {
    "64bit": {
      "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v9.1/windows/pgadmin4-9.1-x64.exe",
      "hash": "729c88032303f848cf8eecc673896c896904f138f13103a4b12b689d4369e181"
    }
  },
  "installer": {
    "args": [
      "/VERYSILENT",
      "/SUPPRESSMSGBOXES",
      "/SP-",
      "/NOCANCEL",
      "/NORESTART",
      "/NOICONS",
      "/DIR=$dir"
    ]
  },
  "uninstaller": {
    "file": "unins000.exe",
    "args": [
      "/VERYSILENT",
      "/SUPPRESSMSGBOXES",
      "/SP-",
      "/NOCANCEL",
      "/NORESTART"
    ]
  },
  "bin": [
    "runtime\\psql.exe",
    "runtime\\pgAdmin4.exe",
    "runtime\\pg_restore.exe",
    "runtime\\pg_dumpall.exe",
    "runtime\\pg_dump.exe"
  ],
  "shortcuts": [
    [
      "runtime\\pgAdmin4.exe",
      "pgAdmin 4"
    ]
  ],
  "checkver": {
    "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
    "regex": "pgAdmin 4 v([\\d.]+)"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x64.exe"
      }
    }
  }
}