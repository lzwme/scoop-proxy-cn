  "hash": "sha1:d42d7719dbbade118ddf1c1868d5745624baae73",
  "depends": "extras/keepass",
  "installer": {
    "script": "Copy-Item \"$dir\\Japanese.lngx\" \"$(appdir keepass $global)\\current\\Languages\""
