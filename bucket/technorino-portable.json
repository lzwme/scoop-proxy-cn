{
  "version": "10608961160",
  "description": "Chat client for twitch.tv. Only nightlies are supported (fork of Chatterino 2).",
  "homepage": "https://github.com/2547techno/technorino",
  "license": "MIT",
  "architecture": {
    "64bit": {
      "url": "https://mirror.ghproxy.com/https://github.com/2547techno/technorino/releases/download/nightly-build/chatterino-windows-x86-64-Qt-6.7.1.zip",
      "hash": "f071db6572e8b2e9c0c6ab6a7f59aaff69c05a5fe3ba637dbdfea1cdd24a0aee",
      "extract_dir": "Chatterino2",
      "pre_install": [
        "Get-ChildItem \"$dir/chatterino.exe\" | Rename-Item -NewName { $_.name.Replace('chatterino', 'technorino-portable') }",
        "$modes = \"$dir/modes\"",
        "$PSDefaultParameterValues['Out-File:Encoding'] = 'utf8'",
        "Invoke-Expression \"'portable' >$(if (Test-Path $modes) { '>' }) `$modes\""
      ]
    }
  },
  "persist": [
    "Logs",
    "Misc",
    "Settings"
  ],
  "shortcuts": [
    [
      "technorino-portable.exe",
      "technorino-portable"
    ]
  ],
  "bin": "technorino-portable.exe",
  "checkver": {
    "script": [
      "(Invoke-RestMethod 'https://api.github.com/repositories/604452497/actions/workflows/build.yml/runs?branch=technorino&status=success').workflow_runs[0].id,",
      "(Invoke-RestMethod https://mirror.ghproxy.com/https://raw.githubusercontent.com/2547techno/technorino/technorino/.github/workflows/build.yml | Select-String 'os: windows-latest\\s+qt-version: ([\\d.]+)').Matches.Groups[1].Value -join ' '"
    ],
    "regex": "(\\d+) (?<qt>[\\d.]+)"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://mirror.ghproxy.com/https://github.com/2547techno/technorino/releases/download/nightly-build/chatterino-windows-x86-64-Qt-$matchQt.zip"
      }
    }
  }
}