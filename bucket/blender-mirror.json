{
  "version": "3.6.1",
  "description": "3D creation suite, using a mirror site for the download for faster speeds",
  "homepage": "https://www.blender.org",
  "license": "GPL-3.0-or-later",
  "architecture": {
    "64bit": {
      "url": "https://mirror.clarkson.edu/blender/release/Blender3.6/blender-3.6.1-windows-x64.zip",
      "extract_dir": "blender-3.6.1-windows-x64"
    }
  },
  "bin": "blender.exe",
  "shortcuts": [
    [
      "blender.exe",
      "Blender"
    ]
  ],
  "checkver": {
    "url": "https://www.blender.org/download/",
    "regex": "blender-([\\w.]+)-windows-x64\\.zip"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://mirror.clarkson.edu/blender/release/Blender$majorVersion.$minorVersion/blender-$version-windows-x64.zip",
        "extract_dir": "blender-$version-windows-x64"
      }
    }
  }
}