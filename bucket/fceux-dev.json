    "identifier": "GPL-2.0"
  },
  "architecture": {
    "64bit": {
      "url": "https://ci.appveyor.com/api/buildjobs/5puvhd0ssjac7mcl/artifacts/fceux-win64.zip",
      "hash": "6c204195c25c55031c502435f99aee1c0092bfaaaa33f43ecf799e98a20c635c",
      "bin": [
        [
          "fceux64.exe",
          "fceux-dev"
        ]
      ],
      "shortcuts": [
        [
          "fceux64.exe",
          "FCEUX (dev)"
        ]
      ]
    },
    "32bit": {
      "url": "https://ci.appveyor.com/api/buildjobs/36grkq8f1lse2noa/artifacts/fceux-win32.zip",
      "hash": "bdc39301d57d67051b2cd1113ea046e4fc0296dc2ea0feb056783edf59f6ca4b",
      "bin": [
        [
          "fceux.exe",
          "fceux-dev"
        ]
      ],
      "shortcuts": [
        [
          "fceux.exe",
          "FCEUX (dev)"
        ]
      ]
    }
  },
  "pre_install": [
    "if (!(Test-Path \"$persist_dir\\fceux.cfg\")) {",
    "    New-Item \"$dir\\fceux.cfg\" -Type File | Out-Null",
    "}"
  ],
  "persist": [
    "cheats",
    "fcs",
    "movies",
    "sav",
    "snaps",
    "fceux.cfg"
  ],
  "checkver": {
    "url": "https://ci.appveyor.com/api/projects/zeromus/fceux",
    "script": [
      "$job32 = json_path $page $.build.jobs[0].jobId",
      "$job64 = json_path $page $.build.jobs[1].jobId",
      "$build = json_path $page $.build.buildNumber",
      "$commit = json_path $page $.build.commitId",
      "Write-Output \"$job32 $job64 $build $commit\""
    ],
    "regex": "(?<job32>[\\w]{16}) (?<job64>[\\w]{16}) (?<build>[\\d]+) (?<commit>[a-f0-9]{8})",
    "replace": "${build}-${commit}"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://ci.appveyor.com/api/buildjobs/$matchJob64/artifacts/fceux-win64.zip"
      },
      "32bit": {
        "url": "https://ci.appveyor.com/api/buildjobs/$matchJob32/artifacts/fceux-win32.zip"
      }
    }
  },
  "_from": "hermanjustnu/scoop-emulators"
}