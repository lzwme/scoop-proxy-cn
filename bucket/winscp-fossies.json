  "pre_install": [
    "if (!(Test-Path \"$persist_dir\\winscp.ini\")) {",
    "    new-item \"$dir\\winscp.ini\" -type file | out-null",
    "    write-output \"[Configuration\\Interface]`r`nRandomSeedFile=.\\winscp.rnd\" | Out-File -encoding \"ASCII\" \"$dir\\winscp.ini\"",
    "}"
  ],
