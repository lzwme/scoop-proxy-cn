      "$extensionFolder = \"$(appdir googlechrome $global)\\current\\User Data\\Default\\Extensions\\$extensionID\\$version\"",
      "Remove-Item \"$extensionFolder\" -Force -Recurse -ErrorAction SilentlyContinue"
    ]
  },
  "checkver": {
    "regex": "Version[^\\d.]+([\\d.]+)",
