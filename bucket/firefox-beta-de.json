  "architecture": {
    "64bit": {
      "url": "https://archive.mozilla.org/pub/firefox/releases/123.0b4/win64/de-DE/Firefox%20Setup%20123.0b4.exe#/dl.7z",
      "hash": "sha512:5eeb7c06240032fee59d181c349b112896f35c1527533302e50769c5119ba3a1aa50bba01ad6ae198172f5ecd543d0fb122e092b6b2aa7533b6dbb98335883da"
    },
    "32bit": {
      "url": "https://archive.mozilla.org/pub/firefox/releases/123.0b4/win32/de-DE/Firefox%20Setup%20123.0b4.exe#/dl.7z",
      "hash": "sha512:5eeb7c06240032fee59d181c349b112896f35c1527533302e50769c5119ba3a1aa50bba01ad6ae198172f5ecd543d0fb122e092b6b2aa7533b6dbb98335883da"
    }
  },
  "extract_dir": "core",
  "bin": [
    [
      "firefox.exe",
      "firefox-beta-de",
      "-profile \"$dir\\profile\""
    ]
  ],
  "shortcuts": [
    [
      "firefox.exe",
      "Firefox Beta (de)",
      "-profile \"$dir\\profile\""
    ]
  ],
  "persist": "profile",
  "checkver": {
    "url": "https://www.mozilla.org/de/firefox/all/",
    "regex": "<option value=\"desktop_beta\">([\\db.]+)"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
        "hash": {
          "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
          "regex": "$sha512\\s+win64/de-DE/$basename"
        }
      },
      "32bit": {
        "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
        "hash": {
          "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
          "regex": "$sha512\\s+win32/de-DE/$basename"
        }
      }
    }
  }
}