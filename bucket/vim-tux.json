    ],
    "xxd.exe"
  ],
  "shortcuts": [
    [
      "gvim.exe",
      "gVim"
    ]
  ],
  "checkver": {
    "regex": "Latest compiled update:</b>\\s(?<y>\\d+)&#45;(?<m>\\d+)&#45;(?<d>\\d+)\\s.*<b>Vim version:</b>\\s+(?<version>[\\d.]+)",
    "replace": "${version}_${y}${m}${d}"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://tuxproject.de/projects/vim/complete-x64.7z"
      },
      "32bit": {
