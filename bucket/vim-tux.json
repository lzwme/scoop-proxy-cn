      "gview",
      "-R"
    ],
    [
      "gvim.exe",
      "evim",
      "-y"
    ],
    [
      "gvim.exe",
      "eview",
      "-Ry"
    ],
    [
      "gvim.exe",
      "rgvim",
      "-Z"
    ],
    [
      "gvim.exe",
      "rgview",
      "-RZ"
    ],
    [
      "gvim.exe",
      "gvimdiff",
