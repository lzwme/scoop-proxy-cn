      "hash": "sha512:c444dd26d158406e353403b96b4706c764ff730c452adead6dcf2a321498e0e75661d18a6691c95f180037b86be5d7dcc0c172e61a3e9c5ae890c95fe9232a8e"
    },
    "32bit": {
