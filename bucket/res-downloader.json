  },
  "architecture": {
    "64bit": {
      "url": "https://github.com/putyy/res-downloader/releases/download/3.0.3/res-downloader_3.0.3_win_x64.exe",
      "hash": "973ab3e4a92f7fcdc445cd8615d6226154ca1b06fbd90e2d3d697f8847879f63"
    },
    "arm64": {
      "url": "https://github.com/putyy/res-downloader/releases/download/3.0.3/res-downloader_3.0.3_win_arm.exe",
      "hash": "eb0b71895693b481c1e04c6fa7bdf771ab42da63a18f7d220a2a68fe68712827"
    }
  },
  "pre_install": "Get-ChildItem \"$dir\\res-downloader*.exe\" | Rename-Item -NewName 'res-downloader.exe'",
  "installer": {
    "script": [
      "# handle runtime cache",
      "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name spc) scripts/AppsUtils.psm1)",
      "Mount-ExternalRuntimeData -Source \"$persist_dir\" -Target \"$env:APPDATA\\res-downloader\"",
      "Remove-Module -Name AppsUtils"
    ]
  },
  "uninstaller": {
    "script": [
      "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name spc) scripts/AppsUtils.psm1)",
      "Dismount-ExternalRuntimeData -Target \"$env:APPDATA\\res-downloader\"",
      "Remove-Module -Name AppsUtils"
    ]
  },
  "shortcuts": [
    [
      "res-downloader.exe",
      "res-downloader"
    ]
  ],
  "checkver": {
    "url": "https://api.github.com/repos/putyy/res-downloader/releases",
    "regex": "/releases/tag/([\\d.]+)"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://github.com/putyy/res-downloader/releases/download/$version/res-downloader_$version_win_x64.exe"
      },
      "arm64": {
        "url": "https://github.com/putyy/res-downloader/releases/download/$version/res-downloader_$version_win_arm.exe"
      }
    }
  }
}