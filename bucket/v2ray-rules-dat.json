  },
  "depends": "v2ray",
  "url": [
    "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/download/202504052212/geoip.dat",
    "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/download/202504052212/geosite.dat"
