  "env_add_path": "bin",
  "env_set": {
    "SPARK_HOME": "$dir"
  },
  "persist": "conf",
  "checkver": {
    "url": "https://spark.apache.org/downloads.html",
    "regex": "version: ([\\d.]+)"
  },
