    }
  },
  "bin": "mpv.com",
  "shortcuts": [
    [
      "mpv.exe",
      "mpv"
    ]
  ],
  "persist": "portable_config",
  "checkver": {
    "url": "https://sourceforge.net/projects/mpv-player-windows/rss?path=/64bit",
