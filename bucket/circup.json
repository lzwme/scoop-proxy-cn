{
  "version": "1.2.3",
  "description": "CircuitPython library updater. ",
  "homepage": "https://github.com/adafruit/circup",
  "license": "MIT",
  "url": "https://ghproxy.net/https://github.com/adafruit/circup/archive/refs/tags/1.2.3.tar.gz",
  "hash": "e0675b59ebd01fe5812b2e62eecb97bedf76745e2db158da0c48bc879e84e402",
  "depends": "python",
  "installer": {
    "script": [
      "Push-Location \"$dir\"",
      "python -m venv circup",
      ".\\circup\\Scripts\\activate",
      "try {",
      "    python -m pip install wheel",
      "    python -m pip install circup",
      "}",
      "finally {",
      "    Remove-Item \"circup-$version\" -Recurse",
      "    deactivate",
      "    Pop-Location",
      "}"
    ]
  },
  "bin": "circup\\Scripts\\circup.exe",
  "checkver": "github",
  "autoupdate": {
    "url": "https://ghproxy.net/https://github.com/adafruit/circup/archive/refs/tags/$version.tar.gz"
  }
}