      "hash": "e453bdab94923579baca1a5d7f969f642e5cdca29f55ec7878839a9b56399699"
    }
  },
  "extract_to": "tmp",
  "installer": {
    "script": [
      "(Get-ChildItem -Directory \"$dir\\tmp\").FullName | % { Move-Item \"$_\\*\" \"$dir\" }",
      "Remove-Item -Recurse \"$dir\\tmp\""
    ]
  },
  "env_add_path": "bin",
  "env_set": {
