  "url": "https://raw.githubusercontent.com/TheDotSource/New-ISOFile/35cb63eec42423cea8861c96166cbdd4a8a8ab6f/New-ISOFile.ps1",
  "hash": "c6283ab9dcb1fb0b9e13620c0c1bec2ebddb8ba7df8b84425714a114c3e4c38d",
  "bin": "New-ISOFile.ps1",
  "checkver": {
    "url": "https://github.com/TheDotSource/New-ISOFile/commits/main/New-ISOFile.ps1.atom",
    "regex": "(?s)>(\\d+)-(\\d+)-(\\d+)T.*?/(?<sha>[0-9a-f]{40})",
    "replace": "0.${1}.${2}.${3}"
  },
  "autoupdate": {
    "url": "https://raw.githubusercontent.com/TheDotSource/New-ISOFile/$matchSha/New-ISOFile.ps1"
  }
}