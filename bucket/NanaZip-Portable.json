  "pre_install": [
    "Remove-Item -Path \"$dir\\*\" -Exclude *.msix -Recurse -Force",
    "Expand-7zipArchive -Path \"$dir\\*$(switch ($architecture) { '64bit' { 'x64' } '32bit' { 'x86' } 'arm64' { 'ARM64' } }).msix\"",
    "Remove-Item -Path \"$dir\\*\" -Include '*.msix', 'Appx*', '?Content*', 'Code*' -Recurse -Force"
  ],
  "bin": [
    [
      "NanaZipC.exe",
      "7z"
    ],
    [
      "NanaZipG.exe",
      "7zG"
    ]
  ],
