{
  "version": "0.9.3",
  "homepage": "https://github.com/clawsoftware/clawPDF",
  "description": "An opens Source Virtual (Network) Printer for Windows that allows you to create PDFs, OCR text, and print images",
  "license": "AGPL-3.0",
  "notes": "You may have to restart your system after installing/uninstalling clawPDF for it to function properly.",
  "url": "https://ghproxy.net/https://github.com/clawsoftware/clawPDF/releases/download/0.9.3/clawPDF_0.9.3_setup.msi#/setup.msi_",
  "hash": "4b8102955e7a75149c45efc968643db87b3aed85753136b186734ee1c614298c",
  "installer": {
    "script": [
      "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }",
      "Start-Process 'msiexec' -Wait -Verb 'RunAs' -Args @('/i', \"$dir\\setup.msi_\", '/qn', '/norestart', '/log', \"$dir\\clawPDF_install.log\")",
      "Set-Content \"$dir\\clawpdf.cmd\" '@\"%ProgramFiles(x86)%\\clawPDF\\clawPDF.exe\"  %*' -Encoding 'utf8'",
      "Set-Content \"$dir\\setuphelper.cmd\" '@\"%ProgramFiles(x86)%\\clawPDF\\SetupHelper.exe\"  %*' -Encoding 'utf8'"
    ]
  },
  "uninstaller": {
    "script": [
      "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }",
      "Start-Process 'msiexec' -Wait -Verb 'RunAs' -Args @('/x', \"$dir\\setup.msi_\", '/qn', '/norestart', '/log', \"$dir\\clawPDF_uninstall.log\")",
      "Get-ChildItem \"$dir\\*.log\" | Move-Item -Destination \"$persist_dir\\MSI_Logs\"",
      "warn 'You may have to restart your system after uninstalling clawPDF to completely remove the virtual printer.'"
    ]
  },
  "bin": [
    "clawpdf.cmd",
    "setuphelper.cmd"
  ],
  "persist": "MSI_Logs",
  "post_install": "Get-ChildItem \"$dir\\*.log\" | Move-Item -Destination \"$persist_dir\\MSI_Logs\"",
  "checkver": "github",
  "autoupdate": {
    "url": "https://ghproxy.net/https://github.com/clawsoftware/clawPDF/releases/download/$version/clawPDF_$version_setup.msi#/setup.msi_"
  }
}