  "bin": [
    [
      "root\\bin\\bash.exe",
      "cygwin",
      "--login -i"
    ]
  ],
  "checkver": {
    "regex": ">release_([\\d.]+)<",
    "url": "https://cygwin.com/git/?p=cygwin-apps/setup.git;a=tags"
