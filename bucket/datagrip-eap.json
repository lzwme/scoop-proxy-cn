  "checkver": {
    "url": "https://data.services.jetbrains.com/products/releases?code=DG&latest=true&platform=zip&type=eap",
    "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
    "replace": "${ver}-${build}"
