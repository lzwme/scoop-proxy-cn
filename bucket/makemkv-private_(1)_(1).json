    "makemkv.exe",
    [
      "makemkvcon64.exe",
      "makemkvcon64"
    ],
    [
      "makemkvcon64.exe",
      "makemkvcon"
    ]
  ],
  "shortcuts": [
    [
      "makemkv.exe",
      "MakeMKV"
    ]
  ]
