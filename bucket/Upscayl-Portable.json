      "Upscayl.exe",
      "Upscayl",
      "--user-data-dir=\"$dir\\User Data\""
    ]
  ],
  "persist": "User Data",
  "checkver": "github",
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://github.com/upscayl/upscayl/releases/download/v$version/Upscayl-$version-win.exe#/dl.7z",
        "hash": {
          "url": "$baseurl/latest.yml",
          "mode": "extract",
          "regex": "(?sm)$basename.*?sha512:\\s*?$base64"
        }
      }
    }
  }
