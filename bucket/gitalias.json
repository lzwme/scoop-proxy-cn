    "url": "https://github.com/GitAlias/gitalias/commits/main.atom",
    "regex": "(?s)>(\\d+)-(\\d+)-(\\d+)T.*?/(?<sha>(?<short>[0-9a-f]{7})[0-9a-f]{33})",
    "replace": "0.${1}.${2}.${3}.${short}"
  },
  "autoupdate": {
    "url": "https://github.com/GitAlias/gitalias/raw/main/gitalias.txt"
  },
