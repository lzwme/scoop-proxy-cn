  "hash": "411d1b39550ac892ed5fa4522d17e3e35833015f4968bad161fd66dc4eb07cac",
  "pre_install": "Rename-Item -Path \"$dir\\gitalias.txt\" -NewName \".gitalias\" -Force | Out-Null",
